(lang dune 3.0)

(name DkStdRestApis_Std)
(version 0.4.0)

; Disable generation of opam files.
; They interfere with DkCoder's internal use of dune (perhaps some new dune flag is needed?).
(generate_opam_files false)

(package
  (name DkStdRestApis_Std)
  (synopsis "REST APIs for well-known REST services")
  (depends
    (ocaml (>= 4.14))
    (json-data-encoding (>= 1.0.1))
    (ptime (>= 1.1.0))
    (ezjsonm (>= 1.3.0))))
